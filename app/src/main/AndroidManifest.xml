<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.obstacles2">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/ic_seagull"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_seagull"
        android:supportsRtl="true"
        android:theme="@style/Theme.Obstacles2">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyDhb6j2y-HT9HEbjIw7AN-BEWC7MpvYhHg"/>

        <activity
            android:name=".Activity_Menu"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameOverActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity_High_Scores"
            android:exported="false"
            android:screenOrientation="portrait" />
        <service
            android:name=".SoundService"
            android:enabled="true" />
    </application>

</manifest>